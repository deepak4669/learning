import java.utill.Scanner;
public class Main205A
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int min=1000000000;
    boolean flag=true;
    for(int i=0;i<n;i++)
    {
      int value=sc.nextInt();
      if(value<min)
      {
        min=value;
      }
      else if(value=min)
      {
        flag=false;
      }
    }
    if(flag)
    {
      System.out.println(min);
    }
    else
    {
      System.out.println("Still Rozdil");
    }
  }
}
        
    
                      