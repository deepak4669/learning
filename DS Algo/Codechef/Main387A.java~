import java.util.Scanner;
public class Main387A
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int a=sc.nextInt();
    int b=sc.nextInt();
    int w=0,d=0,l=0;
    for(int i=1;i<=6;i++)
    {
      int x=Math.abs(i-a);
      int y=Math.abs(i-b);
      if(x>y) w++;
      else if(x<y) l++;
      else d++;
    }
    System.out.println(w+" "+d+" "+l);
  }
}
    