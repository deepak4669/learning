import java.util.Scanner;
public class PERMUT2
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    while(true)
    {
      int n=sc.nextInt();
      if(n==0) break;
      int sum=0;
      for(int i=1;i<=n;i++)
      {
        int a=sc.nextInt();
        sum+=(a-i);
      }
      if(sum==0) System.out.println("ambigous");
      else System.out.println("not ambigous");
    }
  }
}
        