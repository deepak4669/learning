import java.util.Scanner;
public class Main8
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int t=sc.nextInt();
    int i=1;
    int j=1;
    res=0;
    while(i*j!=t)
    {
      for(int j=1;j<=t;j++)
      {
        
        res++;
        if(i*j==t) break;
      }
      i++;
    }
    System.out.println(res);
  }
}
      
      