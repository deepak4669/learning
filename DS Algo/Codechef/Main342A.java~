import java.util.Scanner;
public class Main342A
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    assert n%3==0;
    int ones=0;
    int twos=0;
    int threes=0;
    int fours=0;
    int six=0;
    for(int i=0;i<n;i++)
    {
      int val=sc.nextInt();
      if(val==1)
      {
        ones++;
      }
      else if(val==2)
      {
        twos++;
      }
      else if(val==3)
      {
        threes++;
      }
      else if(val==4)
      {
        fours++;
      }
      else if(val==6)
      {
        six++;
      }
    }
    if(ones!=(n/3)){
      System.out.println(-1);
    }
    else
    {
      if(twos+threes==(n/3))
