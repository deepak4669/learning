import java.util.Scanner;
public class Main478B
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int m=sc.nextInt();
    long max=((n-m+1)*(n-m))/2;
    int n1=n/m;
    int n2=n%m;
    long min=((n2)*(n1+1)*(n1))/2+(m-(n%m))*((n1)*(n1+1))/1;
    System.out.println(min+" "+max);
  }
}