import java.util.Scanner;
public class BEENUMS
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    while(true)
    {
      int n=sc.nextInt();
      if(n==-1) break;
      int m=1;
      while(m<n)
      {
        int r=3*m*(m-1)+1;
        if(r==n)
        {
          break;
        }
      }
      if(m<n) System.out.println("Y");
      else System.out.println("N");
    }
  }
}
        