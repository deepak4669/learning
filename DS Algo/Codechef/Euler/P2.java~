public class P2
{
  public static void main(String[] args)
  {
    int N=400000;
    int i=1;
    int f=fib(1);
    while(f<=N)
    {
      int n=fib(i);
      if(n%2==0) sum+=n;
      i++;
    }
    System.out.println(sum);
  }
  public static int fib(int n)
  {
    if(n<=2) return 1;
    else
    {
      return fib(n-1)+fib(n-2);
    }
  }
     
}

    
      
     