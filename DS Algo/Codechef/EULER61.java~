import java.util.Scanner;
public class EULER61
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int t=sc.nextInt();
    while(t-->0)
    {
      long n=sc.nextLong();
      long ans=(n*n*(n+1)*(n+1))/4-((n)*(n+2)(2*n+1))/6;
      System.out.println(ans);
    }
  }
}