import java.util.Scanner;
public class EULER32
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int t=sc.nextInt();
    while(t-->0)
    {
      long n=sc.nextInt();
      long ans=2;
      for(long i=2;i<=n;i++)
      {
        while(n%i==0)
        {
          n/=i;
          ans=i;
        }
      }
      System.out.println(ans);
    }
  }
}
        
          
        