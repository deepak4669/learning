import java.util.Scanner;
public class Main347A
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int[] a=new int[n];
    for(int i=0;i<n;i+=)
      a[i]=sc.nextInt();
    Arrays.sort(a);
    int max=a[n-1];
    int min=a[0];
    System.out.print(max);
    for(int i=1;i<n-1;i++)
    {
      System.out.print(a[i]);
    }
    System.out.print(min);
  }
}