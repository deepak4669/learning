import java.util.Scanner;
public class Main451A
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int m=sc.nextInt();
    boolean malvika=true;
    while(n>0&&m>0)
    {
      n--;
      m--;
      malvika=false;
    }
    if(malvika) System.out.println("Malvika");
    else
      System.out.println("Akshat");
  }
}
      
  