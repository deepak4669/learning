import java.util.Scanner;
public class AMR12D
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int t=sc.nextInt();
    while(t-->0)
    {
      String a=sc.next();
      String revA="";
      for(int i=a.length-1;i>=0;i--)
      {
        revA+=a.charAt(i);
      }
      if(revA.equals(a)) System.out.println("YES");
      else System.out.println("NO");
    }
  }
}