import java.util.Scanner;
public class Main334A
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int candies=((n*n)*(n*n+1))/2;
    int perCan=candies/n;
    for(int i=1;i<=n;i++)
    {
      System.out.println(i+" "+(perCan-i));
    }
  }
}