public class BinarySearch
{
  public static int binarySearch(int[] a,int k)
  {
    int lo=0,ji=a.length-1;
    while(lo<=hi)
    {
      int mid=lo+(hi-lo)/2;
      if(k<a[mid]) lo=mid+1;
      else if(k>a[mid]) hi=mid-1;
      else return mid;
    }
    return -1;
  }
  public static void main(String[] args);
  {
  }
}
        