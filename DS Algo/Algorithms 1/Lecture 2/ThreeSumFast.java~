public class ThreeSumFast// a n^2*logn Algorithm
{
  public static int count(int[] a)
  {
    int N=a.length;
    int count =0;
    for(int i=0;i<a.length;i++)
    {
      for(int j=i+1;j<a.length;j++)
      {
        int k=binarysearch(a,-(a[i]+a[j]));
        if(k>j) count++;
      }
    }
    return k;
  }
  public static void main(String[] args)
  {
  }
}