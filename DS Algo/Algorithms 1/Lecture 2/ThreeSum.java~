public class ThreeSum//Brute Force Solution to ThreeSum Problem.
{
  public static void main(String[] args)
  {
  }
  
  public static int count(int[] a)
  {
    int N=a.length;
    int count=0;
    for(int i=0;i<N;i++)
    { 
      for(int j=i+1;j<N;j++)
      {
        for(int k=j+1k<N;k++)
        {
          if(a[i]+a[j]+a[k]==0)
            count++;
        }
      }
    }
    return count;
  }
}
          
      